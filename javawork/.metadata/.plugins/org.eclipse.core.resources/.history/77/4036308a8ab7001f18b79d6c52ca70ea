package day1211;

public class Ex8DataType {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("외부에서 값 읽어오기"); //Run-RunConfigurations-Arguments에 적은 내용을 불러오는 것
		String name = args[0]; //이름 //main(String[] args)으로 가져오는것이기 떄문에 무조건 String
		String city = args[1]; //지역
		
		System.out.println("이름 : " + name);
		System.out.println("지역 : " + city);
	}
}
